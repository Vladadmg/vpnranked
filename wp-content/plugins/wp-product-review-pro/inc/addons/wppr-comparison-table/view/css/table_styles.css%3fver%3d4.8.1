
/* Table */

.cwppose_clearfix:after {

  visibility: hidden;

  display: block;

  font-size: 0;

  content: " ";

  clear: both;

  height: 0;

}

.cwppose_hide{
  display: none;
}



.cwppose_reviews_table {
    
    background: #fff;

    width: 100%;

    display: table;

  	border-collapse: collapse !important;

    border: 10px solid #fff!important;

    color:#7f7f7f;
    
    font-size: 14px;

    font-style: normal;

}



.cwppose_reviews_table,

.cwppose_reviews_table td,

.cwppose_reviews_table th {

    vertical-align: middle;

    text-align: center;

}



.cwppose_reviews_table td, 

.cwppose_reviews_table th {

  border: 1px solid #e6e6e6;

  padding: 10px;

}



.cwppose_reviews_table tr:first-child th {

  border-top: 0;

}



.cwppose_reviews_table tr:last-child td {

  border-bottom: 0;

}

.cwppose_reviews_table #cwppose_pimg {
  padding-top:0;
  padding-bottom: 0;
  max-height: 200px;
}



.cwppose_reviews_table tr td:first-child,

.cwppose_reviews_table tr th:first-child {

  border-left: 0;

}



.cwppose_reviews_table tr td:last-child,

.cwppose_reviews_table tr th:last-child {

  border-right: 0;

}



.cwppose_reviews_table tbody tr:nth-child(odd) {

   background-color: #F7F7F7;

}



.cwppose_reviews_table .rating {

  height: 16px;

  width: 80px;

  margin: 0 auto;

  display: block;

  background-image: url(data:image/svg+xml;base64,);

  background-size: contain;

}



.cwppose_reviews_table .rating div{

	background-image: url(data:image/svg+xml;base64,);

	background-size: contain;

  height: 16px;

  display: block;

  max-width: 100%;

  text-indent: 100%;
  white-space: nowrap;
  overflow: hidden;
}



.cwppose_reviews_table .review {

  display: block;

  text-align: center;

  white-space: nowrap;
  
  transition: all 0.5s ease;
}



.cwppose_reviews_table .review:hover {

  color: #3baeda !important;

}



.cwppose_affiliate_button{

  border-radius: 3px;

  background-color: #3baeda;

  line-height: 36px;

  font-size: 12px;

  font-weight: bold;

  padding: 0;

  color: #fff !important;

  text-transform: uppercase;

  text-decoration: none;

  display: block;

  white-space: nowrap;
  
  transition: all 0.5s ease;

}



.cwppose_affiliate_button:hover{

  opacity: 0.8;

}



.cwppose_reviews_table p{

  padding: 0;

  margin: 0 !important;

  line-height: 20px !important;

}



.cwppose_reviews_table a{

  text-decoration: none;

  font-weight: bold;

}



.cwppose_reviews_table h2{

  color: #252b2f;

  text-transform: uppercase;

  margin:0;

  font-size: 18px;

  font-weight: bold;
  
  word-wrap: break-word;
}



.cwppose_reviews_table tr td:first-child {

  font-weight: bold;
  white-space: nowrap;
}





.cwppose_reviews_table th {

  font-size: 12px;

  color:#252b2f;

  font-weight: bold;

  text-transform: uppercase;

  white-space: nowrap;
    
    background: #fff;

}


.cwppose_reviews_table th.header {
  padding-right: 24px;
  background: url(data:image/svg+xml;base64,) no-repeat;
  background-position: right 10px center;
  background-size: auto 14px;
  cursor: pointer;
}

.cwppose_reviews_table th.headerSortUp {
  background-image: url(data:image/svg+xml;base64,);
}

.cwppose_reviews_table th.headerSortDown {
  background-image: url(data:image/svg+xml;base64,);
}


.cwppose_reviews_table .title_option{

  width: 25px;

  position: relative;

  text-align: center;

  margin: 0;

  padding: 0;

  border-right: 1px solid transparent;

}



.cwppose_reviews_table .title_option span{

  -webkit-transform: rotate(-45deg);

  display: block;

  position: absolute;

  width: 100%;

  bottom: 0;

  margin: 24px auto;

  white-space: nowrap;

}



.cwppose_reviews_table .option_thead{ 
  
  width: auto;

}



.cwppose_reviews_table .option_group{ 

  display: block;

  margin-bottom: 10px;

}



.cwppose_reviews_table .option{ 

  display: block;

  float:left;

  line-height: 13px;

  text-align: right;

  overflow: hidden;

  white-space: nowrap;

  font-weight: bold;

}



.cwppose_reviews_table .bar{ 

  width: 70px;

  display: block;

  float:left;

  padding-left: 10px;

  box-sizing: border-box;

  font-size: 10px;

  font-weight: bold;

  color:#000;

  text-align: left;

  line-height: 13px;

}



.cwppose_reviews_table .bar span{

  margin-left: 5px; 

}



.cwppose_reviews_table .grade{

  max-width: 100%;

  background: #ccc;

  display: block;

  height: 13px;

  border-radius: 3px;

  text-align: right;

  float: left;

  color:#fff;

}



.cwppose_reviews_table .grade span{

  margin-right: 5px; 

}



.cwppose_reviews_table .option_group:last-child{ 

  margin-bottom: 0;

}



.cwppose_reviews_table .st-val .option_group{ 

  width: 100%;

}



.cwppose_reviews_table .st-val .option{ 

  width: 50%;

}



.cwppose_reviews_table .st-val .bar{ 

  width: 50%;

}


/* Responsive table */
.stacktable { width: 100%; }
.st-head-row { padding-top: 1em; }
.st-head-row.st-head-row-main { font-size: 1.5em; padding-top: 0; }
.st-key { width: 29%; text-align: right; padding-right: 1%; }
.st-val { width: 69%; padding-left: 1%; }



/* RESPONSIVE EXAMPLE */

.stacktable.large-only { display: table; }
.stacktable.small-only { display: none; }

@media (max-width: 800px) {
  .stacktable.large-only { display: none; }
  .stacktable.small-only { display: table; }
}


/* Admin panel */

.cwppose_clearfix:after {

  visibility: hidden;

  display: block;

  font-size: 0;

  content: " ";

  clear: both;

  height: 0;

}



#cwppose_container {

  margin: 15px;

  position: relative;

  z-index: 0;

}



#cwppose_header {

  height: 70px;

  background:#f9f9f9;

  border: 1px solid #ccc;

  -webkit-border-radius: 6px 6px 0 0;

  -moz-border-radius: 6px 6px 0 0;

  border-radius: 6px 6px 0 0;

  -moz-box-shadow: inset 0 1px 0 #fff;

  -webkit-box-shadow: inset 0 1px 0 #fff;

  box-shadow: inset 0 1px 0 #fff;

}



#cwppose_header .logo {

  float: left;

  margin:10px 20px;

}



#cwppose_header .logo span {

  color:#888888;

}



#cwppose_content {

  min-height: 100%;

  box-sizing: border-box;

  width: 100%;

  padding: 14px;

  font-family: "Lucida Grande", Sans-serif;

  background-color: #FCFCFC;

  border: 1px solid #d8d8d8;

  -moz-box-shadow: inset 0 1px 0 #fff;

  -webkit-box-shadow: inset 0 1px 0 #fff;

  box-shadow: inset 0 1px 0 #fff;

}



#cwppose_content h1,

#cwppose_content h2,

#cwppose_content h3,

#cwppose_content h4,

#cwppose_content h5,

#cwppose_content h6 {

  clear: both;

  font-weight: normal;

  margin: 36px 0 12px;

  color: #2a2a2a;

}



#cwppose_content h1 {

  font-size: 24px;

  line-height: 1.3846153846;

  color: #21759B;

}



#cwppose_content h2 {

  font-size: 24px;

  line-height: 1;

  color: #222;

}



#cwppose_content h3 {

  font-size: 22px;

  line-height: 1.0909090909;

  color: #EC5923;

}



#cwppose_content h4 {

  font-size: 20px;

  line-height: 1.2;

}



#cwppose_content h5 {

  font-size: 18px;

  line-height: 1.3333333333;

}



#cwppose_content h6 {

  font-size: 12px;

  line-height: 1.5;

}



#cwppose_content .alignleft, 

#cwppose_content img.alignleft {

  display: inline;

  float: left;

  margin-right: 1.5em;

}



#cwppose_content .alignright, 

#cwppose_content img.alignright {

  display: inline;

  float: right;

  margin-left: 1.5em;

}



#cwppose_content .aligncenter, 

#cwppose_content img.aligncenter {

  clear: both;

  display: block;

  margin-left: auto;

  margin-right: auto;

}



#cwppose_content .alignnone, 

#cwppose_content img.alignnone {

}



#cwppose_content .controls{

  padding-bottom: 10px;
  float: left;
  width: 45%;

}



#cwppose_content .cwp_textarea, #cwppose_content .cwp_input, #cwppose_content .cwp_select, #cwppose_content .cwp_button{

  -webkit-border-radius: 3px;

